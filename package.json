{
  "name": "bloomwell-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:grants": "jest --config jest.config.grants.js",
    "test:webinars": "jest --config jest.config.webinars.js",
    "test:chat": "jest --config jest.config.chat.js",
    "test:all-features": "npm run test:grants && npm run test:webinars && npm run test:chat",
    "type-check": "tsc --noEmit",
    "db:seed:admin": "node scripts/database/seed-admin.js",
    "db:seed:admin:simple": "node scripts/database/seed-admin-simple.js",
    "db:seed:webinars": "node scripts/database/seed-webinars.js",
    "db:seed:templates": "node scripts/database/seed-templates.js",
    "db:seed:notifications": "node scripts/database/seed-notification-templates.js",
    "db:seed:maintenance": "node scripts/maintenance/seed-maintenance-mode.js",
    "db:create:test-user": "node scripts/database/create-test-user.js",
    "db:clear:test-org": "node scripts/database/clear-test-organization.js",
    "db:fix:trial-dates": "node scripts/database/fix-trial-dates.js",
    "grants:import:ca": "node scripts/database/import-ca-grants.js",
    "grants:sync": "tsx scripts/database/sync-grants-now.ts",
    "grants:check": "node scripts/database/check-grants-count.js",
    "grants:health": "node scripts/database/grant-health-check.js",
    "grants:verify:ca": "node scripts/database/verify-ca-grants.js",
    "grants:cleanup": "node scripts/database/cleanup-expired-grants.js",
    "deploy:setup:admin": "node scripts/deployment/setup-admin-session.js",
    "deploy:setup:ollama": "node scripts/deployment/setup-ollama-cloud.js",
    "deploy:set-feature-flags": "node scripts/deployment/set-feature-flags.js",
    "deploy:staging": "npm run deploy:set-feature-flags -- --env=staging",
    "deploy:production": "npm run deploy:set-feature-flags -- --env=production",
    "deploy:disable-feature": "node scripts/deployment/set-feature-flags.js --disable",
    "deploy:enable-feature": "node scripts/deployment/set-feature-flags.js --enable",
    "deploy:reset-features": "node scripts/deployment/set-feature-flags.js --reset",
    "deploy:rollback": "node scripts/deployment/rollback.js",
    "deploy:rollback:feature": "node scripts/deployment/rollback.js",
    "maintenance:test:ollama": "node scripts/maintenance/test-ollama-cloud.js",
    "maintenance:test:search": "node scripts/maintenance/test-web-search-api.js",
    "ci:verify-features": "node scripts/deployment/verify-feature-status.js",
    "ci:test-feature-isolation": "jest tests/feature-isolation"
