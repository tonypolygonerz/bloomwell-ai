  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.15.0",
    "@react-email/render": "^1.4.0",
    "@stripe/stripe-js": "^7.9.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pdf-parse": "^1.1.5",
    "@types/xml2js": "^0.4.14",
    "adm-zip": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "csv-parser": "^3.2.0",
    "date-fns": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "ollama": "^0.6.0",
    "pdf-parse": "^1.1.1",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "resend": "^6.1.2",
    "stripe": "^18.5.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/adm-zip": "^0.5.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "prisma": "^6.15.0",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
}
